            "source": [
                "function birthdayCakeCandles(candles) {\n    // Write your code here\n    let noc=0;\n    let hieghst=candles[0];\n    for(let candle of candles){\n        if(candle >= hieghst){\n            hieghst = candle;\n        }\n    }\n    for(let candle of candles){\n        if(candle === hieghst){\n            noc++;\n        }\n    }\n\n    return noc;\n}"
