        {
            "language": "javascript",
            "source": [
                "function migratoryBirds(arr) {\n    // Write your code here\n        const allBirds = {};\n        for (let i = 0; i < arr.length; i++) {\n            if (!allBirds[arr[i]]) {\n                allBirds[arr[i]] = 1;\n            } else {\n                allBirds[arr[i]]++;\n            }\n        }\n        const candidates = [];\n        for (let prop in allBirds) {\n            if (allBirds[prop] > 1) {\n                candidates.push(prop);\n            }\n        }\n        let smallestRepeatedId = candidates[0];\n        for (let i = 1; i < candidates.length; i++) {\n            if (allBirds[candidates[i]] > allBirds[smallestRepeatedId]) {\n                smallestRepeatedId = candidates[i];\n            }\n        }\n        return smallestRepeatedId;\n    }"
